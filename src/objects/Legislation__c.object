    <fields>
        <fullName>THOMAS_link__c</fullName>
        <description>Link to THOMAS description of bill.  Only works if Bill Name and Congressional Session are filled in.</description>
        <externalId>false</externalId>
        <formula>if(
  or(isblank(text(Congressional_Session__c)),
      isblank(text( Bill_Prefix__c )),
      isblank(text(Bill_Number__c))
  ),
&quot;Choose a value for Bill Prefix, Bill Number, and Congressional Session&quot;,
HYPERLINK(&quot;http://hdl.loc.gov/loc.uscongress/legislation.&quot; + text(Congressional_Session__c) +  LOWER(text(Bill_Prefix__c)) + text(Bill_Number__c) , &quot;View in THOMAS&quot;, &quot;_blank&quot;))</formula>
        <inlineHelpText>Link to THOMAS description of bill.  Only works if Bill Name and Congressional Session are filled in.</inlineHelpText>
        <label>THOMAS link</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
